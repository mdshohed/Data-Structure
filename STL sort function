#include<bits/stdc++.h>
using namespace std;

bool  myfunc( int a, int b)
{
    return (a>b);
}

int main()
{
    vector<int> v;
    vector<int>::iterator it;
    int n,x;
    for ( int i =0; i<n; i++) {
        cin>>x;
        v.push_back(x);
    }
    
    /// full arry assending order sort....
    sort(v.begin(), v.end());
    
    /// full array disassendig order sort...
    sort(v.begin(), v.end(), myfunc);
    
    /// if only array 3/4 element sort then...
    sort(v.begin(), v.begin()+3);
    
    for ( it = v.begin(), it1=v.end(); it++) {
        cout<<*it<<" ";
    }
    return 0;

}

/// array sort...
#include<iostream>
#include<vector>
#include<iterator>
#include<string>
#include<stdio.h>
#include<algorithm>
using namespace std;
int main()
{
    int ara[100];
    int n;
    cin>>n;
    for ( int i = 0; i<n; i++) {
        cin>>ara[i];
    }

    sort(ara,ara+n);
    for (int i = 0; i<n; i++) {
        cout <<ara[i]<<" ";

    }
    return 0;
}
