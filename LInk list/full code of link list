#include<bits/stdc++.h>
using namespace std;

typedef struct node Node;
struct node{
    int data;
    Node *next;
};

Node *head = NULL;

void append(int item ) {
    Node *temp = new node();
    temp->data = item;
    temp->next = NULL;
    if ( head == NULL) {
        head = temp;
    }
    else {
        Node *t = head;
        while( t->next != NULL) {
            t = t->next;
        }
        t->next = temp;
    }
}

void prepend( int item) {
    Node *temp = new node();
    temp->data = item;
    temp->next = head;
    head = temp;
}

void delete_node ( int x) {
    Node *current_node = head;
    Node *previous_node = NULL;
    while( current_node->data!= x ) {
        previous_node = current_node;
        current_node = current_node->next;
    }
    if ( current_node == head) {
        Node *temp = head;
        head = head->next;
        free(temp);
    }
    else {
        previous_node->next = current_node->next;
        free(current_node);
    }
}

void search_node(int x) {
    Node *temp  = head;
    int y = 0;
    while (temp !=NULL) {
        if ( temp->data == x) {
            cout<<"Yes"<<endl;
            y = 1;
            break;
        }
        temp = temp->next;
    }
    if ( y == 0) {
        cout<<"No"<<endl;
    }
}

void print() {
    Node *temp  = head;
    while (temp != NULL) {
        cout<<temp->data<<" ";
        temp = temp->next;
    }
    cout <<endl;

}

int main() {
    append(30);
    append(40);
    append(10);
    prepend(10);
    prepend(20);
    prepend(40);
    prepend(100);
    print();
    delete_node(30);
    print();
    search_node(100);
    return 0;
}
