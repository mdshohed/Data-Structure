vector :
    ara[5] = {5,7,2,4,9};
    vector<int> v(ara,ara+5);
    push_back()
    find(v.begin(), v.end(), searc_value)
    v.clear()
    v.reverse()
    v.erase()
    sort(v.begin(),v.end())
    
list:
    push_back()
    push_front()
    pop_front()
    pop_back()
    find(v.begin(), v.end(), searc_value)
    v.insert(iterator, value_pass)
    v.clear()
    v.reverse()
    v.erase()

    
